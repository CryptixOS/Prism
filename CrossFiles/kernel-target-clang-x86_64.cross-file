[binaries]
c = ['ccache', 'clang']
cpp = ['ccache', 'clang++']
c_ld = 'lld'
cpp_ld = 'lld'
ar = 'llvm-ar'
strip = 'llvm-strip'
pkg-config = 'pkg-config'
cmake = 'cmake'

[built-in options]
c_args = ['-target', 'x86_64-linux-elf']
cpp_args = ['-target', 'x86_64-linux-elf']
cpp_eh = 'none'
cpp_rtti = 'false'


[host_machine]
system = 'pc'
cpu_family = 'x86_64'
cpu = 'x86_64'
endian = 'little'
