[binaries]
c = ['aarch64-none-elf-gcc']
cpp = ['aarch64-none-elf-g++']
c_ld = 'bfd'
cpp_ld = 'bfd'
ar = 'aarch64-none-elf-ar'
strip = 'aarch64-none-elf-nm'
pkg-config = 'pkg-config'
cmake = 'cmake'

[built-in options]
#c_args = ['-target', 'aarch64-none-elf']
#cpp_args = ['-target', 'aarch64-none-elf']
cpp_eh = 'none'
cpp_rtti = 'false'

[host_machine]
system = 'none'
cpu_family = 'aarch64'
cpu = 'aarch64'
endian = 'little'
